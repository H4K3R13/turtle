{
  "manifest_version": 3,
  "name": "turtle",
  "version": "1.0.0",
  "description": "A bookmark extention",
  "icons": {
    "32": "turtle.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_width": 4000,
    "default_height": 6000
  },
  "permissions": ["scripting"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
